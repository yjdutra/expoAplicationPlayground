{
  "expo": {
    "name": "expoAplicationPlayground",
    "slug": "expoAplicationPlayground",
    "scheme": "expoaplicationplayground",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./icon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.yjdutra.expoAplicationPlayground"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "extra": {
      "eas": {
        "projectId": "acfca4b7-c325-425d-8028-448f9a1abef9"
      }
    },
    "plugins": [
      [
        "./plugins/withAndroidMainActivityAttributes",
        {
          "attributes": [
            {
              "android:name": "android.intent.category.HOME"
            },
            {
              "android:name": "android.intent.category.DEFAULT"
            }
          ]
        }
      ]
    ]
  }
}
